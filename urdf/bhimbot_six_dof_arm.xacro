<?xml version="1.0" ?>

<robot name="BhimBot_five_dof_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Include materials-->
  <material name="Black">
    <color rgba="0.0 0.0 0.0 1.0" />
  </material>

  <material name="Red">
    <color rgba="0.8 0.0 0.0 1.0" />
  </material>

  <material name="White">
    <color rgba="1.0 1.0 1.0 1.0" />
  </material>

  <material name="Blue">
    <color rgba="0.0 0.0 1.0 1.0"/>
  </material>

  <material name="Grey">
    <color rgba="0.5 0.5 0.5 1.0" />
  </material>

  <!-- Include properties -->
  <property name="M_PI" value="3.14159" />

  <property name="base_width" value="0.08" />
  <property name="base_len" value="0.06" />

  <property name="shoulder_pan_width" value="0.08" />
  <property name="shoulder_pan_len" value="0.02" />

  <property name="shoulder_pitch_len" value="0.14" />
  <property name="shoulder_pitch_width" value="0.04" />
  <property name="shoulder_pitch_height" value="0.04" />

  <property name="elbow_pitch_len" value="0.22" />
  <property name="elbow_pitch_width" value="0.04" />
  <property name="elbow_pitch_height" value="0.04" />

  <property name="wrist_pitch_len" value="0.10" />
  <property name="wrist_pitch_width" value="0.04" />
  <property name="wrist_pitch_height" value="0.04" />

  <property name="wrist_roll_width" value="0.06" />
  <property name="wrist_roll_len" value="0.02" />

  <property name="gripper_finger_len" value="0.08" />
  <property name="gripper_finger_width" value="0.02" />
  <property name="gripper_finger_height" value="0.02" />


  <xacro:macro name="inertial_matrix" params="mass">
    <inertial>
      <mass value="${mass}" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0" />
    </inertial>
  </xacro:macro>
  
  <xacro:macro name="transmission_block" params="joint_name">
    <transmission name="tran1">
      <type> transmission_interface/SimpleTransmission  </type>

      <joint name="${joint_name}">
        <hardwareInterface> EffortJointInterface </hardwareInterface>
      </joint>

      <actuator name="motor1">
        <hardwareInterface> EfforJointInterface </hardwareInterface>
        <mechanicalReduction> 1  </mechanicalReduction>
      </actuator>      
    </transmission>
  </xacro:macro>
  <!-- ===============================================================================================================  -->

  <link name="bottom_link">
    <visual>
      <origin xyz="0 0 -0.01" rpy="0 0 0" />
      <geometry>
        <box size="0.6 0.6 0.02" />
      </geometry>
      <material name="White" />
    </visual>

    <collision>
      <origin xyz="0 0 -0.01" rpy="0 0 0" />
      <geometry>
        <box size="0.6 0.6 0.02" />
      </geometry>
    </collision>

    <xacro:inertial_matrix mass="100" />
  </link>
  
  <gazebo reference="bottom_link">
    <material> Gazebo/White</material>
  </gazebo>

  <joint name="bottom_joint" type="fixed">
    <parent link="base_link" />
    <child link="bottom_link" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>

  <!-- ==============================================================================================================  -->
  
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0.03" rpy="0 0 0" />
      <geometry>
        <cylinder radius="${base_width}" length="${base_len}" />
      </geometry>
      <material name="Blue" />
    </visual>

    <collision>
      <origin xyz="0 0 0.03" rpy="0 0 0" />
      <geometry>
        <cylinder radius="${base_width}" length="${base_len}" />
      </geometry>
    </collision>

    <xacro:inertial_matrix mass="1" />
  </link>

  <gazebo reference="base_link">
    <material> Gazebo/Blue </material>
  </gazebo>

  <joint name="shoulder_pan_joint" type="revolute">
    <parent link="base_link" />
    <child link="shoulder_pan_link" />
    <origin xyz="0 0 0.07" rpy="-${M_PI/2} ${M_PI/2} ${M_PI*0}" />
    <axis xyz="-1 0 0" />
    <limit effort="300" velocity="1" lower="-1.57" upper="1.57" />
    <dynamics damping="50" friction="1" />
  </joint>

  <!-- ==============================================================================================================  -->

  <link name="shoulder_pan_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 ${M_PI/2} 0" />
      <geometry>
        <cylinder radius="${shoulder_pan_width}" length="${shoulder_pan_len}" />
      </geometry>
      <material name="Grey" />
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 ${M_PI/2} 0" />
      <geometry>
        <cylinder radius="${shoulder_pan_width}" length="${shoulder_pan_len}" />
      </geometry>
    </collision>

    <xacro:inertial_matrix mass="1" />
  </link>

  <gazebo reference="shoulder_pan_link">
    <material> Gazebo/Grey</material>
  </gazebo>
 
  <joint name="shoulder_pitch_joint" type="revolute">
    <parent link="shoulder_pan_link" />
    <child link="shoulder_pitch_link" />
    <origin xyz="0 0 0" rpy="-${M_PI/2} 0 ${M_PI/2}" />
    <axis xyz="0 1 0" />
    <limit effort="300" velocity="1" lower="-1.57" upper="1.57" />
    <dynamics damping="50" friction="1" />
  </joint>

  <!-- ==============================================================================================================  -->

  <link name="shoulder_pitch_link">
    <visual> 
      <origin xyz="0 0 0.07" rpy="0 ${M_PI/2} 0" />
      <geometry>
        <box size="${shoulder_pitch_len} ${shoulder_pitch_width} ${shoulder_pitch_height}" />
      </geometry>
      <material name="Blue" />
    </visual>

    <collision>
      <origin xyz="0 0 0.07" rpy="0 ${M_PI/2} 0" />
      <geometry>
        <box size="${shoulder_pitch_len} ${shoulder_pitch_width} ${shoulder_pitch_height}" />
      </geometry>
    </collision>

    <xacro:inertial_matrix mass="1" />
  </link>

  <gazebo reference="shoulder_pitch_link">
    <material> Gazebo/Blue </material>
  </gazebo>

  <joint name="elbow_pitch_joint" type="revolute">
    <parent link="shoulder_pitch_link" />
    <child link="elbow_pitch_link" />
    <origin xyz="0 0 0.14" rpy="0 0 ${M_PI/2}" />
    <axis xyz="1 0 0" />
    <limit effort="300" velocity="1" lower="-1.57" upper="1.57" />
    <dynamics damping="50" friction="1" />
  </joint>

  <!-- ==============================================================================================================  -->
  
  <link name="elbow_pitch_link">
    <visual>
      <origin xyz="0 0 0.11" rpy="0 ${M_PI/2} 0" />
      <geometry>
        <box size="${elbow_pitch_len} ${elbow_pitch_width} ${elbow_pitch_height}" />
      </geometry>
      <material name="Grey" />
    </visual>

    <collision>
      <origin xyz="0 0 0.11" rpy="0 ${M_PI/2} 0" />
      <geometry>
        <box size="${elbow_pitch_len} ${elbow_pitch_width} ${elbow_pitch_height}" />
      </geometry>
    </collision>

    <xacro:inertial_matrix mass="1" />
  </link>
  
  <gazebo reference="elbow_pitch_link">
    <material> Gazebo/Grey </material>
  </gazebo>

  <joint name="wrist_pitch_joint" type="revolute">
    <parent link="elbow_pitch_link" />
    <child link="wrist_pitch_link" />
    <origin xyz="0 0 0.22" rpy="0 0 0" />
    <axis xyz="1 0 0" />
    <limit effort="300" velocity="1" lower="-1.57" upper="1.57" />
    <dynamics damping="50" friction="1" />
  </joint>

  <!-- ==============================================================================================================  -->

  <link name="wrist_pitch_link">
    <visual>
      <origin xyz="0 0 0.05" rpy="0 ${M_PI/2} 0" />
      <geometry>
        <box size="${wrist_pitch_len} ${wrist_pitch_width} ${wrist_pitch_height}" />
      </geometry>
      <material name="Blue" />
    </visual>

    <collision>
      <origin xyz="0 0 0.05" rpy="0 ${M_PI/2} 0" />
      <geometry>
        <box size="${wrist_pitch_len} ${wrist_pitch_width} ${wrist_pitch_height}" />
      </geometry>
    </collision>

    <xacro:inertial_matrix mass="1" />
  </link>
 
  <gazebo reference="wrist_pitch_link">
    <material> Gazebo/Blue </material>
  </gazebo>

  <joint name="wrist_roll_joint" type="revolute">
    <parent link="wrist_pitch_link" />
    <child link="wrist_roll_link"/>
    <origin xyz="0 0 0.10" rpy="-${M_PI/2} ${M_PI/2} 0" />
    <axis xyz="1 0 0" />
    <limit effort="300" velocity="1" lower="-1.57" upper="1.57" />
    <dynamics damping="50" friction="1" />
  </joint>

  <!-- ==============================================================================================================  -->

  <link name="wrist_roll_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 ${M_PI/2} 0"/>
      <geometry>
        <cylinder radius="${wrist_roll_width}" length="${wrist_roll_len}" />
      </geometry>
      <material name="Grey" />
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 ${M_PI/2} 0"/>
      <geometry>
        <cylinder radius="${wrist_roll_width}" length="${wrist_roll_len}" />
      </geometry>
    </collision>

    <xacro:inertial_matrix mass="1" />
  </link>

  <gazebo reference="wrist_roll_link">
    <material> Gazebo/Grey </material>
  </gazebo>

  <joint name="gripper_left_finger_joint" type="prismatic">
    <parent link="wrist_roll_link" />
    <child link="gripper_left_finger_link" />
    <origin xyz="0 0 0" />
    <axis xyz="0 1 0" />
    <limit effort="100" velocity="1" lower="0" upper="0.05" />
    <dynamics damping="50" friction="1" />
  </joint>

  <!-- ==============================================================================================================  -->

  <link name="gripper_left_finger_link">
    <visual>
      <origin xyz="-0.04 -0.03 0" />
      <geometry>
        <box size="${gripper_finger_len} ${gripper_finger_width} ${gripper_finger_height}" />
      </geometry>
      <material name="Blue" />
    </visual>

    <collision>
      <origin xyz="-0.04 -0.03 0" />
      <geometry>
        <box size="${gripper_finger_len} ${gripper_finger_width} ${gripper_finger_height}" />
      </geometry>
    </collision>

    <xacro:inertial_matrix mass="1" />
  </link>

  <gazebo reference="gripper_left_finger_link">
    <material> Gazebo/Blue </material>
  </gazebo>
  
  <joint name="gripper_right_finger_joint" type="fixed">
    <parent link="wrist_roll_link" />
    <child link="gripper_right_finger_link" />
    <origin xyz="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <!-- ==============================================================================================================  -->

  <link name="gripper_right_finger_link">
    <visual>
      <origin xyz="-0.04 0.03 0" />
      <geometry>
        <box size="${gripper_finger_len} ${gripper_finger_width} ${gripper_finger_height}" />
      </geometry>
      <material name="Blue" />
    </visual>

    <collision>
      <origin xyz="-0.04 0.03 0" />
      <geometry>
        <box size="${gripper_finger_len} ${gripper_finger_width} ${gripper_finger_height}" />
      </geometry>
    </collision>

    <xacro:inertial_matrix mass="1" />
  </link>

  <gazebo reference="gripper_right_finger_link">
    <material> Gazebo/Blue </material>
  </gazebo>



  <!-- Transmissions for ros_control -->
  <xacro:transmission_block joint_name="shoulder_pan_joint" />
  <xacro:transmission_block joint_name="shoulder_pitch_joint" />
  <xacro:transmission_block joint_name="elbow_pitch_joint" />
  <xacro:transmission_block joint_name="wrist_pitch_joint" />
  <xacro:transmission_block joint_name="wrist_roll_joint" />
  <xacro:transmission_block joint_name="gripper_left_finger_joint" />

  <!-- Gazebo ros_control plugin-->
  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
     <robotNamespace> /bhimbot </robotNamespace>
     <robotSimType> gazebo_ros_control/DefaultRobotHWSim </robotSimType>
    </plugin>
  </gazebo>

</robot>
